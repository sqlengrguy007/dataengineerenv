{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-databootcamp"
		},
		"linkedService1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linkedService1'"
		},
		"ls_azuresqldatacamp_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_azuresqldatacamp'"
		},
		"ls_dedsqldatacamp_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_dedsqldatacamp'"
		},
		"linkedService1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datastagingadls.dfs.core.windows.net/"
		},
		"ls_azuresqldatacamp_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "svrbootcamp.database.windows.net"
		},
		"ls_azuresqldatacamp_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dbdatabootcamp"
		},
		"ls_azuresqldatacamp_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"ls_dedsqldatacamp_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "svrbootcamp.database.windows.net"
		},
		"ls_dedsqldatacamp_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "deddwhbootcamp"
		},
		"ls_dedsqldatacamp_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-databootcamp_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"linkedService1_accountKey": {
						"value": "[parameters('linkedService1_accountKey')]"
					},
					"ls_azuresqldatacamp_password": {
						"value": "[parameters('ls_azuresqldatacamp_password')]"
					},
					"ls_dedsqldatacamp_password": {
						"value": "[parameters('ls_dedsqldatacamp_password')]"
					},
					"linkedService1_properties_typeProperties_url": {
						"value": "[parameters('linkedService1_properties_typeProperties_url')]"
					},
					"ls_azuresqldatacamp_properties_typeProperties_server": {
						"value": "[parameters('ls_azuresqldatacamp_properties_typeProperties_server')]"
					},
					"ls_azuresqldatacamp_properties_typeProperties_database": {
						"value": "[parameters('ls_azuresqldatacamp_properties_typeProperties_database')]"
					},
					"ls_azuresqldatacamp_properties_typeProperties_userName": {
						"value": "[parameters('ls_azuresqldatacamp_properties_typeProperties_userName')]"
					},
					"ls_dedsqldatacamp_properties_typeProperties_server": {
						"value": "[parameters('ls_dedsqldatacamp_properties_typeProperties_server')]"
					},
					"ls_dedsqldatacamp_properties_typeProperties_database": {
						"value": "[parameters('ls_dedsqldatacamp_properties_typeProperties_database')]"
					},
					"ls_dedsqldatacamp_properties_typeProperties_userName": {
						"value": "[parameters('ls_dedsqldatacamp_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}